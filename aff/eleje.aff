n [^y]	>	$1fenn-   $2fenn-[PREF]+
n y	>	$1fenn    $2fenn[PREF]+ @	# marad > fennmarad
n y	>	$1fönn    $2fönn[PREF]+ @	# marad > fönnmarad
n [^y]	>	$1fönn-   $2fönn-[PREF]+
